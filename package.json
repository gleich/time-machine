{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "time-machine",
  "title": "time-machine",
  "description": "View status of Time Machine backups",
  "icon": "command-icon.png",
  "author": "matt",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show Backup Status",
      "description": "View status of the current Time Machine backup",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.2",
    "@raycast/utils": "^1.0.7",
    "execa": "^6.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
